{
    "manifest_version": 2,
    "name": "Mrfylke auto-login",
    "version": "0.5.0",
    "description": "Script som logger deg automatisk inn p√• mrfylke sitt internett.",
    "author": "Lars",

	"browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
	"content_scripts": [
    {
      "matches": ["https://login.mrfylke.no/*", "http://login.mrfylke.no/*"],
      "js": ["jquery-2.1.4.min.js","script.js"],
      "run_at": "document_end"
    }
    ],
	"permissions": [
    "tabs",
    "storage"
	]
}